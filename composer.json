{
    "name": "phpdb2unit/db2unit",
    "description": "PHPUnit extension for Db2 database interaction testing",
    "type": "library",
    "keywords": [
        "database",
        "testing",
        "xunit",
        "db2"
    ],
    "homepage": "https://github.com/diggex/db2unit",
    "authors": [
        {
            "name": "Wang Zhong Hao",
            "email": "wzhhao@cn.ibm.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "phpunit/phpunit": "^7.0",
        "symfony/yaml": "^3.0 || ^4.0",
        "ext-pdo": "*",
        "ext-simplexml": "*"
    },
    "autoload": {
        "classmap": [
            "src/"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.1-dev"
        }
    }
}
